{
  "name": "react-docs-renderer",
  "version": "1.0.1",
  "description": "A react library to render PDF",
  "type": "module",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Ar-mane",
    "email": "abderrahmane.arache@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ar-mane/react-docs-renderer"
  },
  "workspaces": [
    "lib/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write ."
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "lib": "^4.3.3",
    "prettier": "2.7.1",
    "tsup": "6.2.3",
    "turbo": "1.4.7",
    "typescript": "4.8.3"
  },
  "packageManager": "pnpm@7.16.1"
}
