{
  "name": "react-docs-renderer",
  "version": "1.0.1",
  "description": "A react library to render PDF",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Ar-mane",
    "email": "abderrahmane.arache@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ar-mane/react-docs-renderer"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "lint": "eslint"
  },
  "keywords": [
    "document",
    "pdf",
    "react-docs-renderer",
    "react-pdf",
    "react-pdf-viewer",
    "renderer",
    "viewer"
  ],
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "autoprefixer": "^10.4.12",
    "pdfjs-dist": "^2.16.105",
    "postcss": "^8.4.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-zoom-pan-pinch": "^2.1.3",
    "styled-components": "^5.3.6",
    "@vitejs/plugin-react": "2.1.0",
    "tslib": "2.4.0",
    "tsup": "6.2.3",
    "typescript": "4.8.3"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "pdfjs-dist": "^2.16.105",
    "react-dom": "^18.1.0"
  },
  "bundleDependencies": true
}
